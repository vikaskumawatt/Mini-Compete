{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "tsconfig.json",
    "assets": ["**/*.prisma"],
    "watchAssets": true
  },
  "generateOptions": {
    "spec": false,
    "flat": true
  },
  "entryFile": "main"
}
